
@{
    ViewBag.Title = "Info";
}

<table border="1" cellpadding="10" align="center" style="text-align:center;">
    <th>@Resource.Queue</th>
    <th>@Resource.DesProc</th>
    <th>@Resource.Agree</th>
    <th>@Resource.UpFac</th>
    <tr>
        <td>@ViewBag.q</td>
        <td>@ViewBag.d</td>
        <td>@ViewBag.ag</td>
        <td>@ViewBag.f</td>
    </tr>
</table>

<table border="1" align="center" style="text-align:center;">
    <th>@Resource.Designer</th>
    <th>@Resource.Obj</th>
    <th>@Resource.CodeRo</th>
    <th>@Resource.CodeRu</th>
    <th>@Resource.Link</th>
    <th>@Resource.Queue</th>
    <th>@Resource.DesProc</th>
    <th>@Resource.Agree</th>
    <th>@Resource.UpFac</th>
    @for(int i = 0; i < ViewBag.len; i++)
    {
        <tr>
            @for (int j = 0; j < 9; j++)
            {
                if (j == 4)
                {
                    <td><a href="@ViewBag.ar[i, j]">@ViewBag.ar[i, j]</a></td>
                }
                else
                {
                    <td>@ViewBag.ar[i, j]</td>
                }
            }
        </tr>
    }
</table>

