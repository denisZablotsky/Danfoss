
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="windows-main">
    
    <div id="go-to-charts">
       @Html.ActionLink(Resource.CreateGraphBtn, "GetNav", "Chart")
    </div>
    
    <div class="window-files">
        <div id="current-files">
            <h4>Текущие документы</h4>
            @{ 
                Html.RenderAction("GetCurrentFiles", "Home");
            }
        </div>
        <h4>Важные документы</h4>
        <div id="files-list">
            @{
                Html.RenderAction("GetFileList", "Home");
            }
        </div>
    </div>
    <div class="window-files">
        <h4>Информационные письма</h4>
        <div id="letters-list-div">
            @{
                Html.RenderAction("GetLetterList", "Letter");
            }
            </div>
        </div>
    <div class="window-reports">
        <h4>Собрания группы БТП</h4>
        <div id="report-list-div">
        @{
            Html.RenderAction("GetReportList", "Report");
        }
        </div>   
    </div>
</div>